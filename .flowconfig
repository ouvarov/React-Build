[ignore]
.*/__tests__/.*
<PROJECT_ROOT>/\(node_modules\|build\|stories\)/.*

[include]

[libs]
flow-typed

[lints]

[options]
suppress_comment= \\(.\\|\n\\)*\\$FlowIgnore
module.file_ext=.js
module.file_ext=.jsx
module.file_ext=.json
module.file_ext=.css
module.file_ext=.less
module.name_mapper.extension='css' -> 'empty/object'
module.name_mapper.extension='less' -> 'empty/object'

[strict]
unclear-type
